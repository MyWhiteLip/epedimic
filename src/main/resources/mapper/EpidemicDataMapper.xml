<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.seu.mapper.EpidemicDataMapper">
    <insert id="addEpidemicData" parameterType="com.example.seu.entity.EpidemicData">
        insert into mydata.epidemic_data(province_id,city_id,confirm,asymptomatic,death,cured,confirm_increase,asymptomatic,time)
        values(#{provinceId},#{cityId},#{confirm},#{asymptomatic},#{death},#{cured},#{confirmIncrease},#{asymptomaticIncrease},#{time})
    </insert>
    <delete id="deleteEpidemicData">
        delete from mydata.epidemic_data where id=#{id}
    </delete>
</mapper>
